 #include<iostream>
using namespace std;

int main(){

        int category;
    cout<<"Enter a category of hurricane 1through5:"<< endl;

    cin>> category;

    if(category==1){

    cout<<" The windspeed is 74-95 mph"<<endl;

    }

  else  if (category==2){

    cout<<" The windspeed is 96-110 mph" << endl;
    }   

  else  if (category==3){
  
  
   cout<< "The windspeed is 11-130 mph"<< endl;
    }

   else if(category==4){

    cout<< "The windspeed is 131-155 mph"<<endl;
    }

   else if (category==5){

    cout<< " Windspeed exceeds 155 mph"<< endl;
    }

    return 0;
}
                   
                                                             
